# GPT-5 Trader Agent 配置

# ==================== Agent 身份 ====================
MODEL_ID=gpt-5
MODEL_NAME=GPT-5

# ==================== AI 模型配置 ====================
AI_PROVIDER=openai
AI_MODEL=gpt-4

# ==================== 交易所配置 ====================
# 留空则使用全局配置（.env 文件）
EXCHANGE=
TRADING_MODE=

# OKX 配置（留空则使用全局配置）
OKX_API_KEY=
OKX_API_SECRET=
OKX_API_PASSWORD=
OKX_USE_SANDBOX=

# ==================== Agent 行为配置 ====================
INTERVAL_SECONDS=180
MAX_HISTORY_LENGTH=1
BYPASS_RISK_CONTROL=true
